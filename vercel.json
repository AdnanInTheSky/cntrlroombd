{
  "outputDirectory": ".",
  "routes": [
     
    {
      "src": "/((?!api|_next|.*\\.).*)",
      "headers": {
        "cache-control": "public, max-age=300"
      }
    },
     
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "Referrer-Policy": "strict-origin-when-cross-origin"
      }
    }
  ]
}